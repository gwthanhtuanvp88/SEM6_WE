@model IEnumerable<ClaimReport.Models.Academyyear>

@{
    ViewBag.Title = "Academyyear";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Academyyear</h2>

<p>
    @Html.ActionLink("Create New", "CreateAcademyYear")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.startReportDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.closureReportDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.closureEvidenceDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.startReportDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.closureReportDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.closureEvidenceDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.status)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditAcademyYear", new { id=item.id }) |
            @Html.ActionLink("Delete", "DeleteAcademyYear", new { id=item.id })
        </td>
    </tr>
}

</table>
